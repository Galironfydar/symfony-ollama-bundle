{
  "name": "galironfydar/ollama-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle for integrating Ollama AI models into your application",
  "keywords": ["symfony", "bundle", "ollama", "ai", "machine learning"],
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "symfony/config": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/framework-bundle": "^7.0",
    "symfony/http-client": "^7.0",
    "symfony/yaml": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "symfony/phpunit-bridge": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Galironfydar\\OllamaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Galironfydar\\OllamaBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
